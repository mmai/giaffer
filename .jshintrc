{
    // ECMAScript 5
    "es5"          : false,     // Allow ECMAScript 5 syntax.
    "strict"       : true,     // Require `use strict` pragma in every file.
    "globalstrict" : true,     // Allow global "use strict" (also enables 'strict').

    "immed"        : true,
    "sub"          : true,
    "devel"        : true,
    "forin"        : true,
    "curly"        : true,
    "browser"      : true,
    "loopfunc"     : true,
    "trailing"     : true,
    "eqeqeq"       : true,
    "indent"       : 4,
    "newcap"       : true,
    "noarg"        : true,
    "node"         : true,
    "noempty"      : true,
    "undef"        : true,
    "white"        : false,
    "predef": [
        "google",
        "angular",
        "_"
    ]
}
